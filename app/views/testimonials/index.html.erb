	<%= link_to "Create new testimonial", new_testimonial_path %><br>
<% end %>

<% @testims.each do |t| %>
  <strong>Customer name: </strong><%= t.cust_name %><br>
  <strong>Customer description: </strong><%= t.cust_desc %><br>
  <strong>Stars: </strong><%= t.stars %><br>
