<div class="mdl-card mdl-cell mdl-shadow--3dp mdl-cell--12-col">
  <div class="mdl-card__title mdl-color--yellow-300">
    <h2 class="mdl-card__title-text">Admin Actions</h2>
  </div>
  <div class="mdl-card__supporting-text  mdl-card--border">
    Create New User
    <%= link_to "Create", new_user_registration_path, class: "mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" %>
  </div>
  <div class="mdl-card__supporting-text  mdl-card--border">
    Create New Content
    <%= link_to "Create", new_content_path, class: "mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" %>
  </div>
  <div class="mdl-card__supporting-text  mdl-card--border">
    Create New Service
    <%= link_to "Create", new_service_path, class: "mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" %>
  </div>
  <div class="mdl-card__supporting-text  mdl-card--border">
    Create New Walker
    <%= link_to "Create", new_walker_path, class: "mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" %>
  </div>
  <div class="mdl-card__supporting-text  mdl-card--border">
    Create New Testimonial
    <%= link_to "Create", new_testimonial_path, class: "mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" %>
  </div>
</div>
